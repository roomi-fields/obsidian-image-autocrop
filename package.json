{
  "name": "obsidian-image-autocrop",
  "version": "1.0.0",
  "description": "Automatically crop, square, and resize images in specified folders",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "image",
    "crop",
    "resize"
  ],
  "author": "Claude",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.20.0",
    "obsidian": "^1.5.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "sharp": "^0.33.2"
  }
}
